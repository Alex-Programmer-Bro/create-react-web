{
  "name": "create-react-web",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.7.0",
  "description": "_description_",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "crw": "./dist/index.cjs"
  },
  "scripts": {
    "build": "unbuild && npm run copyTmp",
    "dev": "unbuild --stub && npm run copyTmp",
    "lint": "eslint .",
    "test": "sudo vitest run",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "copyTmp": "copy-folder ../template dist/template"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.6",
    "@antfu/utils": "^0.7.6",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.7",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.7.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "copy-folder-util": "^1.1.1",
    "fs-extra": "^11.1.1"
  }
}
